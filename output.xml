<?xml version="1.0" encoding="utf-8"?>
<database name="company">
	<table name="employee">
		<column name="number" value="primary_key"></column>
		<column name="first_name"></column>
		<column name="middle_name"></column>
		<column name="last_name"></column>
		<column name="salary"></column>
		<column name="address"></column>
		<column name="gender"></column>
		<column name="date_of_birth"></column>
		<column name="department_number" value="foreign_key"></column>
		<column name="supervisor" value="foreign_key"></column>
	</table>
	<table name="project">
		<column name="number" value="primary_key"></column>
		<column name="name"></column>
		<column name="location"></column>
		<column name="department_number" value="foreign_key"></column>
	</table>
	<table name="department">
		<column name="number" value="primary_key"></column>
		<column name="name"></column>
		<column name="employee_number" value="foreign_key"></column>
		<column name="start_date"></column>
	</table>
	<table name="dependent">
		<column name="dependent_number" value="primary_key"></column>
		<column name="first_name"></column>
		<column name="gender"></column>
		<column name="date_of_birth"></column>
		<column name="relationship"></column>
		<column name="employee_number" value="foreign_key"></column>
	</table>
	<table name="work">
		<column name="employee_number" value="primary_key"></column>
		<column name="project_number" value="primary_key"></column>
		<column name="hours_per_week"></column>
	</table>
	<table name="location">
		<column name="location" value="primary_key"></column>
		<column name="department_number" value="primary_key"></column>
	</table>
</database>